    "@appium/base-plugin": "^2.2.22",
    "@appium/types": "^0.14.1",
    "@devicefarmer/adbkit": "^3.2.5",
    "@types/appium-adb": "^9.10.4",
    "@types/node-persist": "^3.1.5",
    "appium-adb": "^11.0.3",
    "appium-chromedriver": "^5.6.19",
    "appium-ios-device": "^2.7.6",
    "async-lock": "^1.2.8",
    "async-wait-until": "^2.0.12",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "download": "^8.0.0",
    "express": "^4.17.3",
    "fs-extra": "^11.1.1",
    "get-port": "^5.1.1",
    "http-proxy-agent": "7.0.0",
    "http-proxy-middleware": "^3.0.0-beta.1",
    "https-proxy-agent": "7.0.2",
    "ip": "^1.1.8",
    "jsonschema": "1.4.1",
    "lodash": "^4.17.21",
    "lokijs": "^1.5.12",
    "node-abort-controller": "^3.1.1",
    "node-cache": "^5.1.2",
    "node-persist": "^3.1.3",
    "node-schedule": "^2.1.1",
    "node-simctl": "^7.3.13",
    "normalize-url": "6.1.0",
    "nyc": "^15.1.0",
    "ora": "5.4.1",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.5.4",
    "tcp-port-used": "^1.0.2",
