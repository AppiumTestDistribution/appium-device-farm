  },
  "peerDependencies": {
    "appium": "^2.2.1"
  },
  "devDependencies": {
    "@appium/docutils": "^0.4.11",
    "@appium/fake-driver": "^5.2.10",
    "@appium/plugin-test-support": "^0.3.23",
    "@babel/core": "7.23.6",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@types/async-lock": "1.4.2",
    "@types/chai": "4.3.10",
    "@types/chai-as-promised": "^7.1.8",
    "@types/express": "4.17.21",
    "@types/fs-extra": "^11.0.0",
    "@types/ip": "^1.1.2",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "4.14.202",
    "@types/lokijs": "1.5.14",
    "@types/mocha": "9.1.1",
    "@types/node-schedule": "1.3.2",
    "@types/sinon": "^17.0.2",
    "@types/sinon-chai": "^3.2.12",
    "@types/tcp-port-used": "1.0.3",
    "@types/uuid": "8.3.4",
    "@types/yargs": "17.0.29",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@wdio/types": "^8.24.2",
    "appium-uiautomator2-driver": "^2.32.3",
    "appium-xcuitest-driver": "^5.7.0",
    "babel-eslint": "10.1.0",
    "chai": "4.3.10",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "7.0.4",
    "js-yaml": "^4.1.0",
    "lint-staged": "15.0.2",
    "mocha": "9.2.2",
    "prettier": "^3.0.3",
    "ramda": "0.29.1",
    "sinon": "17.0.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "^4.5.0",
    "wait-on": "7.0.1",
    "webdriverio": "^8.20.4"
  },
  "appium": {
    "pluginName": "device-farm",
    "mainClass": "DevicePlugin",
    "scripts": {
      "install-go-ios": "./lib/scripts/install-go-ios.js"
    },
    "schema": {
      "$schema": "http://json-schema.org/draft-07/schema",
      "additionalProperties": false,
      "description": "Appium configuration schema for the Device Farm plugin.",
      "properties": {
        "platform": {
          "type": "string"
        },
        "androidDeviceType": {
          "type": "string",
          "default": "both"
        },
        "simulators": {
          "type": "array"
        },
        "iosDeviceType": {
          "type": "string",
          "default": "both"
        },
        "hub": {
          "type": "string"
        },
        "remoteMachineProxyIP": {
          "type": "string"
        },
        "adbRemote": {
          "type": "array"
        },
        "skipChromeDownload": {
          "type": "boolean"
        },
        "maxSessions": {
          "type": "number"
        },
        "cloud": {
          "type": "object"
        },
        "derivedDataPath": {
          "type": "object"
        },
        "emulators": {
          "type": "array"
        },
        "proxy": {
          "type": "object"
        },
        "deviceAvailabilityTimeoutMs": {
          "type": "number"
        },
        "deviceAvailabilityQueryIntervalMs": {
          "type": "number"
        },
        "sendNodeDevicesToHubIntervalMs": {
          "type": "number"
        },
        "checkStaleDevicesIntervalMs": {
          "type": "number"
        },
        "checkBlockedDevicesIntervalMs": {
          "type": "number"
        },
        "newCommandTimeoutSec": {
          "type": "number"
        },
        "bindHostOrIp": {
          "type": "string"
        }
      },
      "title": "Appium device farm plugin",
      "type": "object"
    }
  },
  "engines": {
    "node": "^14.17.0 || ^16.13.0 || >=18.0.0",
    "npm": ">=8"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npm run prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "files": [
    "lib",
    "scripts"
  ],
  "typedoc": {
    "entryPoint": "lib/src/index.js"
  }
}
