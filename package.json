{
  "name": "appium-device-plugin",
  "version": "1.0.0-beta.3",
  "description": "An appium 2.0 plugin that manages and create driver session on available devices",
  "main": "./lib/index.js",
  "scripts": {
    "test": "mocha --require esm test/*.spec.js",
    "build": "babel src -d lib",
    "prepublish": "babel src -d lib",
    "lint": "eslint '**/*.js' --fix",
    "prettier": "prettier '**/*.js' --write --single-quote",
    "install-plugin": "npm run build && appium plugin install --source=local /Users/saikrisv/git/appium-device-plugin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SrinivasanTarget/appium-device-plugin/"
  },
  "contributors": [
    {
      "name": "Saikrishna",
      "email": "saikrishna321@yahoo.com"
    },
    {
      "name": "Srinivasan Sekar",
      "email": "srinivasan.sekar1990@gmail.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SrinivasanTarget/appium-device-plugin/issues"
  },
  "homepage": "https://github.com/SrinivasanTarget/appium-device-plugin#readme",
  "dependencies": {
    "@appium/base-plugin": "^1.6.4",
    "appium-adb": "^8.11.0",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "node-simctl": "^6.4.1",
    "node-schedule": "^2.0.0",
    "async-lock": "^1.2.8",
    "appium-ios-device": "^1.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.21",
    "prettier": "^2.0.5",
    "sinon": "^10.0.0",
    "mocha": "^8.3.2",
    "esm": "^3.2.25",
    "chai": "^4.1.0"
  },
  "appium": {
    "pluginName": "device-manager",
    "mainClass": "DevicePlugin"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npm run prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "lib"
  ]
}
